algoritmo "exercicio 5"
// Função :
// Autor :
// Data : 16/11/2022
// Seção de Declarações 
var
numero:real
dobro:real
terco:real
inicio
escreva("Qual o numero?")
leia(numero)
dobro <- numero * 2
terco <- numero / 3
escreval("O dobro de ",numero," é ",dobro)
escreva("A terça parte de ",numero," é ",terco)
fimalgoritmo
