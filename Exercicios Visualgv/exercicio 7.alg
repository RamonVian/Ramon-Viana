algoritmo "exercicio 7"
// Função :
// Autor :
// Data : 16/11/2022
// Seção de Declarações 
var
numero:real
dolar:real

inicio
escreva("Quantos reais voce possui atualmente?")
leia(numero)
dolar <- numero / 3.45

escreval("Atualmente voce possui ",numero," reais e com esse valor é possiel comprar ",dolar," dolares")

fimalgoritmo
